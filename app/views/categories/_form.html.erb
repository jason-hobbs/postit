<%= form_for(@category) do |f| %>
<%= render 'shared/errors', object: @category %>

  <div class="col-xs-4 well">
    <%= f.label "Category Title", class: "bold" %><br>
    <%= f.text_field :name, :size=>"50", class: "form-control" %>
    <br>
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<% end %>
